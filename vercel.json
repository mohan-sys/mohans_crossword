{
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/build/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
    
  
}